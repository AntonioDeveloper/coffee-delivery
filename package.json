{
  "name": "coffee-delivery",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint-fix-all": "eslint src --ext .ts,.tsx --fix",
    "server": "npx json-server --watch src/database/products.json",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "start-yarn": "yarn run server & yarn run dev"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.1.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.25.0",
    "json-server": "^0.17.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}